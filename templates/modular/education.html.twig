<div class="avoid_pagebreak">
<h4>{{ header.title }}</h4>

{{ content }}

{% if header.education %}
<div class="edu_surround">
  <div class="edu animated">
    {% for item in header.education %}
      <div class="edu_item">
        <div class="date">{{ item.date }}</div>
        <div class="topic">{{ item.topic }}</div>
        <div class="school">{{ item.school }}</div>
      </div>
    {% endfor %}
  </div>
</div>
{% endif %}
</div>
